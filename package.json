{
  "name": "leave-tracker-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "generate-secret": "node scripts/generate-secret.js",
    "db:seed": "tsx prisma/seed.ts",
    "test:auth": "npx tsx test-scripts/test-authentication.ts",
    "test:setup": "npm run db:seed && npm run test:auth",
    "diagnose": "tsx scripts/diagnose-api.ts",
    "fix:server": "bash fix-server.sh",
    "test:api": "npx tsx test-scripts/test-api-endpoints.ts",
    "test:leave": "npx tsx test-scripts/test-leave-crud.ts",
    "fix:types": "npx tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules .next-lock",
    "clean:install": "npm run clean && npm install",
    "dev:clean": "./kill-port.sh && rm -rf .next && next dev --port 3000",
    "dev:force": "./kill-port.sh && PORT=3000 next dev --port 3000",
    "start:dev": "./start-dev.sh",
    "kill:port": "./kill-port.sh",
    "clean:cache": "rm -rf .next",
    "fix:500": "rm -rf .next && npm run dev",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:security": "jest --testPathPattern=security",
    "test:performance": "jest --testPathPattern=performance",
    "test:toil": "jest --testPathPattern=toil",
    "test:auth-utils": "jest --testPathPattern=auth-utils",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:performance",
    "prod:check": "node scripts/production-startup.js",
    "prod:start": "node scripts/production-startup.js --start",
    "prod:build": "NODE_ENV=production npm run build",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "readiness:check": "curl -f http://localhost:3000/api/readiness || exit 1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^0.5.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.542.0",
    "next": "^15.5.1",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-email": "^4.2.8",
    "react-hook-form": "^7.62.0",
    "resend": "^6.0.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "axios-cookiejar-support": "^6.0.4",
    "eslint": "^8",
    "eslint-config-next": "15.1.3",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "playwright": "^1.49.1",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tough-cookie": "^6.0.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
