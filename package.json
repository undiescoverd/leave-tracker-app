{
  "name": "leave-tracker-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-secret": "node scripts/generate-secret.js",
    "db:seed": "tsx prisma/seed.ts",
    "test:auth": "npx tsx test-scripts/test-authentication.ts",
    "test:setup": "npm run db:seed && npm run test:auth",
    "diagnose": "tsx scripts/diagnose-api.ts",
    "fix:server": "bash fix-server.sh",
    "test:api": "npx tsx test-scripts/test-api-endpoints.ts",
    "test:leave": "npx tsx test-scripts/test-leave-crud.ts",
    "fix:types": "npx tsc --noEmit",
    "clean": "rm -rf .next node_modules .next-lock",
    "clean:install": "npm run clean && npm install",
    "dev:clean": "./kill-port.sh && rm -rf .next && next dev --port 3000",
    "dev:force": "./kill-port.sh && PORT=3000 next dev --port 3000",
    "start:dev": "./start-dev.sh",
    "kill:port": "./kill-port.sh",
    "clean:cache": "rm -rf .next",
    "fix:500": "rm -rf .next && npm run dev",
    "test:unit": "jest"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.14.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "next": "^15.5.1",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "15.1.3",
    "jest": "^30.1.1",
    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
